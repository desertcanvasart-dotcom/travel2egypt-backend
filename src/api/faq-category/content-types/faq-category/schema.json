{
    "kind": "collectionType",
    "collectionName": "faq_categories",
    "info": {
      "singularName": "faq-category",
      "pluralName": "faq-categories",
      "displayName": "FAQ Category"
    },
    "options": {
      "draftAndPublish": true
    },
    "pluginOptions": {
      "i18n": {
        "localized": true
      }
    },
    "attributes": {
      "name": {
        "type": "string",
        "required": true,
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "slug": {
        "type": "uid",
        "targetField": "name",
        "required": true
      },
      "faqs": {
        "type": "relation",
        "relation": "manyToMany",
        "target": "api::faq.faq",
        "mappedBy": "faq_categories"
      }
    }
  }