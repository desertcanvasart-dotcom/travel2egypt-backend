{
    "kind": "collectionType",
    "collectionName": "dynasties",
    "info": {
      "singularName": "dynasty",
      "pluralName": "dynasties",
      "displayName": "Dynasty",
      "description": "Egyptian royal dynasties from 1st to 31st"
    },
    "options": {
      "draftAndPublish": true
    },
    "pluginOptions": {
      "i18n": {
        "localized": true
      }
    },
    "attributes": {
      "name": {
        "type": "string",
        "required": true,
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "slug": {
        "type": "uid",
        "targetField": "name",
        "required": true
      },
      "order_number": {
        "type": "integer",
        "required": true,
        "min": 1,
        "max": 1000
      },
      "time_period": {
        "type": "string",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "period_start": {
        "type": "integer"
      },
      "period_end": {
        "type": "integer"
      },
      "overview": {
        "type": "richtext",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
  
      "pharaohs": {
        "type": "relation",
        "relation": "oneToMany",
        "target": "api::historical-king.historical-king",
        "mappedBy": "dynasty"
      },
  
      "queens": {
        "type": "relation",
        "relation": "oneToMany",
        "target": "api::queen.queen",
        "mappedBy": "dynasty"
      },
      "tombs": {
        "type": "relation",
        "relation": "oneToMany",
        "target": "api::tomb.tomb",
        "mappedBy": "dynasty"
      },
      "pyramids": {
        "type": "relation",
        "relation": "oneToMany",
        "target": "api::pyramid.pyramid",
        "mappedBy": "dynasty"
      },
  
      "archaeological_sources": {
        "type": "richtext",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "timeline": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.timeline-entry"
      },
      "fact_box": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.fact-box"
      },
      "seo_title": {
        "type": "string",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "seo_description": {
        "type": "text",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      }
    }
  }
  