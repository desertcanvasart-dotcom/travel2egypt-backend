{
  "kind": "collectionType",
  "collectionName": "tour_types",
  "info": {
    "singularName": "tour-type",
    "pluralName": "tour-types",
    "displayName": "Tour Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "tours": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tour.tour",
      "mappedBy": "tour_types"
    }
  }
}