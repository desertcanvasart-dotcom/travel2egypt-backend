{
    "kind": "collectionType",
    "collectionName": "gods",
    "info": {
      "singularName": "god",
      "pluralName": "gods",
      "displayName": "God or Goddess",
      "description": "Egyptian deities with mythology and worship details"
    },
    "options": {
      "draftAndPublish": true
    },
    "pluginOptions": {
      "i18n": {
        "localized": true
      }
    },
    "attributes": {
      "name": {
        "type": "string",
        "required": true,
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "slug": {
        "type": "uid",
        "targetField": "name",
        "required": true
      },
      "cult_centers": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.cult-center"
      },
      "realm_domain": {
        "type": "string",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "symbols": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.fact-box"
      },
      "depictions": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.fact-box"
      },
      "parentage": {
        "type": "text",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "myths": {
        "type": "richtext",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "associated_festivals": {
        "type": "richtext",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "worship_history": {
        "type": "richtext",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "images_gallery": {
        "type": "media",
        "multiple": true,
        "allowedTypes": ["images"],
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "fact_box": {
        "type": "component",
        "repeatable": true,
        "component": "wiki.fact-box"
      },
      "related_temples": {
        "type": "relation",
        "relation": "manyToMany",
        "target": "api::temple.temple",
        "inversedBy": "worshipped_deities"
      },
      "related_articles": {
        "type": "relation",
        "relation": "manyToMany",
        "target": "api::wiki-article.wiki-article",
        "mappedBy": "related_gods"
      },
      "seo_title": {
        "type": "string",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      },
      "seo_description": {
        "type": "text",
        "pluginOptions": {
          "i18n": {
            "localized": true
          }
        }
      }
    }
  }
  